{
    "testTimeout": 20000,
    "rootDir": "../..",
    "modulePaths": ["."],
    "testEnvironment": "node",
    "testMatch": ["<rootDir>/test/e2e/**/*.e2e.spec.ts"],
    "collectCoverage": true,
    "coverageDirectory": "../coverage/e2e",
    "collectCoverageFrom": ["./e2e"],
    "coverageThreshold": {
        "global": {
            "branches": 90,
            "functions": 90,
            "lines": 90,
            "statements": 90
        }
    },
    "moduleFileExtensions": ["js", "ts", "json"],
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    }
}
